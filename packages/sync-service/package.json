{
  "name": "@sophistree/sync-service",
  "version": "0.1.1",
  "description": "Synchronizes clients for collab editing",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build-watch": "tsc --watch",
    "create-table": "node bin/create-table.js",
    "dev": "concurrently \"npm run build-watch\" \"npm run start-watch\"",
    "format-check": "prettier --check .",
    "format-fix": "prettier --write .",
    "get-current-version": "node -p \"require('./package.json').version\"",
    "lint": "eslint . --ext .ts",
    "lint-fix": "eslint . --ext .ts --fix",
    "start": "node dist/index.js",
    "start-watch": "nodemon --watch dist dist/index.js",
    "test": "echo No tests at the moment.",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@automerge/automerge-repo": "^2.0.0-alpha.23",
    "@automerge/automerge-repo-network-websocket": "^2.0.0-alpha.23",
    "automerge-repo-storage-postgres": "^0.0.1",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "pg": "^8.13.1",
    "ws": "^8.18.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  }
}
