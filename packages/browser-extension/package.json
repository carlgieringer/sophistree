{
  "name": "sophistree-browser-extension",
  "version": "0.1.0",
  "description": "A browser extension for mapping arguments appearing in web pages",
  "main": "index.js",
  "scripts": {
    "build-dev": "NODE_ENV=development node esbuild.config.mjs",
    "build-dev-watch": "NODE_ENV=development node esbuild.config.mjs --watch",
    "build-prod": "NODE_ENV=production node esbuild.config.mjs",
    "check-all": "time (npm run type-check && npm run lint && npm run format-check && npm test) || { echo 'Checks failed'; exit 1; }",
    "extension:build-dev-watch": "npm run build-dev-watch",
    "format-fix": "prettier --write --ignore-path .gitignore .",
    "format-check": "prettier --check --ignore-path .gitignore .",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint src/ --ext .js,.jsx,.ts,.tsx  --fix",
    "package-extension": "npm run build-prod && npm run zip-extension",
    "test": "jest",
    "test-ci": "npm run test",
    "test-watch": "jest --watch",
    "test-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "type-check": "tsc --noEmit",
    "zip-extension": "node bin/zip-extension.js"
  },
  "keywords": [
    "argument mapping"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.7",
    "@sentry/browser": "^8.33.1",
    "classnames": "^2.5.1",
    "cytoscape": "^3.30.2",
    "cytoscape-context-menus": "^4.2.1",
    "cytoscape-elk": "^2.2.0",
    "esbuild-plugin-flow": "github:dalcib/esbuild-plugin-flow",
    "immer": "^10.1.1",
    "lodash.debounce": "^4.0.8",
    "react": "^18.3.1",
    "react-art": "^18.3.1",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.3.1",
    "react-native-paper": "^5.12.5",
    "react-native-safe-area-context": "^4.11.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "^0.19.12",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "tapestry-highlights": "file:../tapestry-highlights",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-typescript": "^7.24.7",
    "@types/chrome": "^0.0.270",
    "@types/cytoscape-context-menus": "^4.1.4",
    "@types/jest": "^29.5.13",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^22.5.4",
    "@types/react": "^18.3.5",
    "@types/react-cytoscapejs": "^1.2.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "babel-jest": "^29.7.0",
    "esbuild": "^0.23.1",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2"
  }
}
