{
  "manifest_version": 3,
  "name": "Sophistree{{#unless prod}} (Dev){{/unless}}",
  "version": "{{version}}",
  "description": "A browser extension for mapping arguments appearing in web pages",
  "key": "",
  "permissions": [
    "contextMenus",
    "identity",{{#unless prod}}
    "management",{{/unless}}
    "scripting",
    "sidePanel",
    "storage",
    "webNavigation"
  ],
  "action": {
    "default_title": "Sophistree{{#unless prod}} (Dev){{/unless}}"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "oauth2": {
    "client_id": "521682983614-3mic45o5a0816d1513ls6emeug19as4v.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "icons": {
    "32": "logo-32.png",
    "512": "logo-512.png"
  }
}
